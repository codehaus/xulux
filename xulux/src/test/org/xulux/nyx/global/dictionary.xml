<?xml version="1.0"?>
<!-- $Id: dictionary.xml,v 1.3 2003-07-10 22:40:22 mvdb Exp $ -->
<dictionary>
   <prefix-mappings>
     <prefix name="Test">
       <!-- 
            if discovery is true, it will find the fields automatically
            and you don't have to specify them manually
        -->
       <bean discovery="true">org.xulux.nyx.global.DictionaryBean</bean>
     </prefix>
     <!--
     <prefix name="Contained">
       <bean discovery="true">org.xulux.nyx.global.DictionarySubBean</bean>
     </prefix>
     -->
     
     <!-- Manual field definitiation -->
     <prefix name="Manual">
       <bean>org.xulux.nyx.global.DictionaryBean</bean>
       <fields>
         <field name="name"/>
         <field name="street" alias="straat"/>
         <field name="city" alias="plaats"/>
       </fields>
     </prefix>
   </prefix-mappings>
</dictionary>