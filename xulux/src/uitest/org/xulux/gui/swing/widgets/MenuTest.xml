<part name="MenuTest" prefix="menu" application="true">
  <field type="window" name="WindowTest1">
    <title>MenuTest</title>
    <icon>resources/Car.ico</icon>
    <size>300,300</size>
    <autosize>true</autosize>
    <field type="menubar" name="MenuBar">
      <field type="menu" name="File">
        <text>File</text>
        <field type="menuitem" name="exit">
          <text>Exit</text>
          <defaultaction>cancel</defaultaction>
        </field>
      </field>
      <field type="menu" name="Help">
        <text>Help</text>
        <field type="menuitem" name="About">
          <text>About</text>
        </field>
      </field>
    </field>
  </field>
</part>