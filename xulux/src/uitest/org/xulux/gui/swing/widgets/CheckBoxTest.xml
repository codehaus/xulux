<?xml version="1.0"?>
<!-- $Id: CheckBoxTest.xml,v 1.4 2005-02-18 09:10:46 mvdb Exp $ -->
<part name="CheckBoxForm" prefix="CheckBoxBean">
  <field type="window" name="Window">
    <title>CheckboxTest</title>
    <icon>resources/Car.ico</icon>
    <size>300,300</size>
    <field type="checkbox" name="foo" use="foo">
      <size>100,21</size>
      <position>10,10</position>
      <text>Foo</text>
       <required-background-color>FF0000</required-background-color>
       <default-background-color>FF0000</default-background-color>
    </field>
    <field type="checkbox" name="bar" use="bar">
       <size>100,21</size>
       <position>10, 40</position>
       <text>Bar</text>
       <required-background-color>FF0000</required-background-color>
    </field>
    <field type="checkbox" name="strfoo" use="strfoo">
      <size>100,21</size>
      <position>10,70</position>
      <text>strFoo</text>
      <rules>
        <rule>org.xulux.gui.rules.CheckBoxTestRule</rule>
      </rules>
    </field>
    <field type="checkbox" name="strbar" use="strbar">
       <size>100,21</size>
       <position>10,100</position>
       <text>strBar</text>
    </field>
    <field type="checkbox" name="disabled">
       <size>150,21</size>
       <position>10,130</position>
       <text>I am disabled</text>
       <enabled>false</enabled>
    </field>
    <field type="button" name="disableButton">
       <size>100,21</size>
       <position>170,130</position>
       <text>Enable</text>
       <rules>
         <rule>org.xulux.gui.rules.CheckBoxTestRule</rule>
       </rules>
    </field>
    <field type="checkbox" name="hidden">
       <size>150,21</size>
       <position>10,160</position>
       <text>I am visible</text>
    </field>
    <field type="button" name="hiddenButton">
       <size>100,21</size>
       <position>170,160</position>
       <text>Hide</text>
       <rules>
         <rule>org.xulux.gui.rules.CheckBoxTestRule</rule>
       </rules>
    </field>
  </field>
</part>
