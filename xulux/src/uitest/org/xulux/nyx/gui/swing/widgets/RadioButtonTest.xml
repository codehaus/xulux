<part name="RadioButtonForm" prefix="RadioButtonBean">
  <field type="window" name="Window">
    <title>RadioButtonBean</title>
    <icon>resources/Car.ico</icon>
    <size>300,300</size>
    <autosize>true</autosize>
    <field type="radiobutton" name="radio:foo">
      <size>100,21</size>
      <position>10,10</position>
      <text>Foo</text>
    </field>
    <field type="radiobutton" name="radio:bar">
       <size>100,21</size>
       <position>10, 40</position>
       <text>Bar</text>
    </field>
    <field type="buttongroup" name="buttongroup">
    <field type="radiobutton" name="group1">
      <size>100,21</size>
      <position>10,70</position>
      <text>Group1</text>
    </field>
    <field type="radiobutton" name="group2">
       <size>100,21</size>
       <position>120, 70</position>
       <text>Group2</text>
    </field>
    </field>
  </field>
</part>
